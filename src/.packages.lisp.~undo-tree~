(defparameter *watch-thread* nil)
(defparameter *watching* nil)

(defun %now () (get-universal-time))

(defun reload-lumen ()
  \"Recompile + recharge le système Lumen via ASDF.\"
  (asdf:load-system :lumen :force t))

(defun remount-app ()
  \"Recompose la pile et remonte les routes de myapp.
  Adapte si tu renommes les fonctions.\"
  ;; 1) nettoyer routes si besoin
  (when (fboundp 'lumen.core.router:clear-routes)
    (lumen.core.router:clear-routes))
  ;; 2) (re)définir les routes de l'app
  (when (fboundp 'myapp::%define-routes)
    (myapp::%define-routes))
  ;; 3) recomposer la pile
  (when (fboundp 'myapp::%compose-pipeline)
    (myapp::%compose-pipeline))
  t)

(defun refresh (&key restart-server)
  \"Recharge le code + remonte l'app. Optionnellement redémarre le serveur.\"
  (format t \"~&[dev] Refresh start...~%\")
  (reload-lumen)
  (remount-app)
  (when restart-server
    (ignore-errors (myapp:halt))
    (myapp:boot))
  (format t \"[dev] Refresh done.~%\")
  t)
